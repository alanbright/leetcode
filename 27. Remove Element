/**
 * @param {number[]} nums
 * @param {number} val
 * @return {number}
 */
var removeElement = function(nums, val) {
    for(var i=nums.length-1; i>= 0; i--){
        //console.log("i", i, "nums[i]",nums[i]);        
        if(nums[i] == val){
            nums.splice(i, 1);
        }
        //console.log(nums)
    }
    return nums.length 
};
