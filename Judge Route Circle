/**
Question:
 https://goo.gl/DA9kuJ
 
 * @param {string} moves
 * @return {boolean}
 count the U char should equal with D
 count the L char should equal with R
 
 reference:
 https://stackoverflow.com/questions/881085/count-the-number-of-occurrences-of-a-character-in-a-string-in-javascript
 */
var judgeCircle = function(moves) {
    Ucount = moves.split("U").length -1
    Dcount = moves.split("D").length -1
    Lcount = moves.split("L").length -1
    Rcount = moves.split("R").length -1
    if(Ucount == Dcount && Lcount == Rcount){
        return true
    }else{
        return false
    }
};
