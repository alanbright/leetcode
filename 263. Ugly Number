var isUgly = function(num) {
    if (num == 0) return false; //avoid endless loop
    while(num %2 ==0){    //remove 2 factor
        num /= 2;
    }
    while(num %3 ==0){
        num /= 3;
    }
    while(num %5 ==0){
        num /= 5;
    }
    return num == 1 
    
};

/*

https://leetcode.com/problems/ugly-number/discuss/69214/2-4-lines-every-language

*/
